{
  "name": "@skypilot/steampress",
  "version": "0.5.9-alpha",
  "description": "Parse, filter, and transform messy spreadsheets into clean object data",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && yarn run compile-ts",
    "check-types": "tsc",
    "generate-typings": "tsc --project tsconfig.generate-typings.json",
    "prepublishOnly": "yarn run check-types && yarn run lint --quiet && yarn test && yarn run build && yarn run generate-typings",
    "test": "jest",
    "all-ci-checks": "yarn run check-types && yarn run lint --quiet && yarn test && yarn run build",
    "compile-ts": "babel ./src --out-dir ./lib --extensions .ts --ignore '**/*.test.ts'",
    "lint": "eslint --cache '**/*.{js,ts}'"
  },
  "keywords": [
    "file",
    "conversion",
    "converter",
    "excel",
    "json",
    "parse",
    "spreadsheet",
    "transform"
  ],
  "author": {
    "name": "William Thorsen",
    "email": "wthorsen@skypilot.dev"
  },
  "license": "ISC",
  "devDependencies": {
    "@skypilot/toolchain": "^0.4.17-alpha"
  },
  "dependencies": {
    "@skypilot/sugarbowl": "^0.10.9",
    "convert-excel-to-json": "^1.6.1"
  },
  "files": [
    "/lib"
  ]
}
