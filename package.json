{
  "name": "@skypilot/steampress",
  "version": "0.5.5-alpha",
  "description": "Parse, filter, and transform messy spreadsheets into clean object data",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && babel ./src -d ./lib --extensions .ts --ignore '**/*.test.ts'",
    "check-types": "tsc",
    "generate-typings": "tsc --project tsconfig.generate-typings.json",
    "prepublishOnly": "npm run build && npm run generate-typings"
  },
  "keywords": [
    "file",
    "conversion",
    "converter",
    "excel",
    "json",
    "parse",
    "spreadsheet",
    "transform"
  ],
  "author": "William Thorsen",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.2.1",
    "eslint-plugin-jest": "^22.15.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@skypilot/sugarbowl": "^0.10.0",
    "convert-excel-to-json": "^1.6.1"
  },
  "files": [
    "/lib"
  ]
}
