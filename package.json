{
  "name": "@skypilot/steampress",
  "version": "0.5.6-alpha",
  "description": "Parse, filter, and transform messy spreadsheets into clean object data",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && babel ./src -d ./lib --extensions .ts --ignore '**/*.test.ts'",
    "check-types": "tsc",
    "generate-typings": "tsc --project tsconfig.generate-typings.json",
    "prepublishOnly": "yarn check-types && yarn test && yarn run build && yarn run generate-typings",
    "test": "jest"
  },
  "keywords": [
    "file",
    "conversion",
    "converter",
    "excel",
    "json",
    "parse",
    "spreadsheet",
    "transform"
  ],
  "author": "William Thorsen",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jest": "^23.0.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@skypilot/sugarbowl": "^0.10.3",
    "convert-excel-to-json": "^1.6.1"
  },
  "files": [
    "/lib"
  ]
}
